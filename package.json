{
  "name": "questioner-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "babel-node app.js",
    "dev": "nodemon npm start",
    "lint": "node_modules/.bin/eslint src/v1/\"**/*.js\" src/tests/**.spec.js",
    "test": "cross-env NODE_ENV=test nyc --require babel-register mocha src/v1/tests/**.spec.js --exit",
    "migrate:refresh": "cross-env NODE_ENV=test babel-node src/database/runMigrations.js down && cross-env NODE_ENV=test babel-node src/database/runMigrations.js up && cross-env NODE_ENV=test babel-node src/database/runMigrations.js seed",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.4.0",
    "make-runnable": "^1.3.6",
    "moment": "^2.23.0",
    "morgan": "~1.9.0",
    "pg": "^7.8.0"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "debug": "~2.6.9",
    "eslint": "^5.11.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "expect": "^23.6.0",
    "express-validator": "^5.3.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9",
    "nyc": "^13.1.0",
    "supertest": "^3.3.0",
    "webpack": "^4.28.4"
  },
  "engines": {
    "node": "10.15.0",
    "npm": "6.4.1"
  }
}
